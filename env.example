# VTX AI Phone System - 环境变量配置模板
# 复制此文件为 .env 并填入实际配置值

# =============================================================================
# VTX服务器配置
# =============================================================================
VTX_SERVER=core1-us-lax.myippbx.com
VTX_PORT=5060
VTX_DOMAIN=aiker.myippbx.com
VTX_DID=14088779998

# =============================================================================
# 分机配置
# =============================================================================
# 分机101配置
EXTENSION_101_USERNAME=your_extension_username
EXTENSION_101_PASSWORD=your_extension_password
EXTENSION_101_DESCRIPTION=AI Assistant Extension

# 分机102配置（可选）
# EXTENSION_102_USERNAME=your_extension_username_2
# EXTENSION_102_PASSWORD=your_extension_password_2
# EXTENSION_102_DESCRIPTION=AI Assistant Extension 2

# =============================================================================
# AI配置
# =============================================================================
# OpenAI配置
OPENAI_API_KEY=your_openai_api_key_here
LLM_PROVIDER=openai
LLM_MODEL=gpt-4o-mini
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=150

# 语音识别配置
STT_PROVIDER=whisper
WHISPER_MODEL=base
WHISPER_LANGUAGE=zh

# 语音合成配置
TTS_PROVIDER=edge-tts
TTS_VOICE=zh-CN-XiaoxiaoNeural
TTS_RATE=1.0

# =============================================================================
# 网络配置
# =============================================================================
SIP_PORT=5060
RTP_PORT_START=10000
RTP_PORT_END=10500
USE_STUN=true
STUN_SERVERS=stun.l.google.com:19302,stun1.l.google.com:19302

# =============================================================================
# 系统配置
# =============================================================================
# 日志配置
LOG_LEVEL=INFO
LOG_FILE=logs/vtx_system.log

# 录音配置
RECORDING_ENABLED=true
RECORDING_PATH=recordings/

# 通话配置
MAX_CONCURRENT_CALLS=10
CALL_TIMEOUT_SECONDS=300
VAD_THRESHOLD=0.5
AUDIO_BUFFER_SIZE=8000

# 监控配置
MONITORING_ENABLED=true
PROMETHEUS_PORT=9090
HEALTH_CHECK_INTERVAL=30

# =============================================================================
# 高级配置（可选）
# =============================================================================
# 自定义API端点
# OPENAI_API_BASE=https://api.openai.com/v1

# 代理配置（如果需要）
# HTTP_PROXY=http://proxy.example.com:8080
# HTTPS_PROXY=http://proxy.example.com:8080

# 调试模式
DEBUG=false

# 性能配置
WORKER_THREADS=4
MAX_MEMORY_USAGE=512MB 